<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true">
		<shape>
			<gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" />
			<stroke android:width="1.0dip" android:color="#ffd2d7ea" />
			<corners android:radius="5.0dip" />
		</shape>
	</item>
	<item android:state_focused="true">
		<shape>
			<gradient android:startColor="#ffffffff" android:endColor="#ffffffff" android:angle="270.0" />
			<stroke android:width="1.0dip" android:color="#ffd2d7ea" />
			<corners android:radius="5.0dip" />
		</shape>
	</item>
	<item>
		<shape>
			<gradient android:startColor="#BCFFFFFF" android:endColor="#BCFFFFFF" android:angle="180.0" />
			<stroke android:width="1.0dip" android:color="#ffd2d7ea" />
			<corners android:radius="5.0dip" />
		</shape>
	</item>
</selector>
